<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragmentListNotes">

    <fragment
        android:id="@+id/fragmentListNotes"
        android:name="com.joao01sb.tarefas.ui.fragment.FragmentListNotes"
        android:label="fragment_list_notes"
        tools:layout="@layout/fragment_list_notes" >
        <action
            android:id="@+id/go_notes_for_noteDetails"
            app:destination="@id/fragmentDetailsNote"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/go_notes_for_editeNote"
            app:destination="@id/fragmentEditeNote"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/fragmentEditeNote"
        android:name="com.joao01sb.tarefas.ui.fragment.FragmentEditeNote"
        android:label="fragment_edite_note"
        tools:layout="@layout/fragment_edite_note" >
        <argument
            android:name="tarefa"
            app:argType="com.joao01sb.tarefas.model.Tarefa"
            app:nullable="true"
            android:defaultValue="@null" />
        <action
            android:id="@+id/go_noteEdite_for_notes"
            app:destination="@id/fragmentListNotes"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/fragmentDetailsNote"
        android:name="com.joao01sb.tarefas.ui.fragment.FragmentDetailsNote"
        android:label="FragmentDetailsNote"
        tools:layout="@layout/fragment_details_note">
        <action
            android:id="@+id/go_noteDetails_for_noteEdite"
            app:destination="@id/fragmentEditeNote"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <argument
            android:name="note"
            app:argType="com.joao01sb.tarefas.model.Tarefa" />
    </fragment>
</navigation>